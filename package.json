        "@turf/boolean-point-in-polygon": "^6.5.0",
        "cheap-ruler": "^3.0.2",
        "deck.gl": "^8.9.35",
        "geolib": "^3.3.4",
        "mapbox-gl": "^3.2.0",
        "maplibre-gl": "^4.1.2",
        "react-map-gl": "^7.1.7"
    },
    "peerDependencies": {
